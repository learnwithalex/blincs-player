{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es6"],
      "jsx": "react",
      "module": "commonjs",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist"
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  